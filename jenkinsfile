@Library('my_shared_lib') _

pipeline {
    agent any 
    stages {

        stage('Git Checkout'){
            steps {
                script{
                    gitCheckout(
                        branch: '*/master',
                        url: 'https://github.com/karthi770/Jenkins_shared_library.git'
                    )
                }
            }
        }

        stage('Unit test maven'){
                    steps {
                        script{
                            mvnTest()
                        }
                    }
                }
    }
}